{
  "name": "bytestash",
  "description": "A React and node.js app that stores code snippets",
  "author": "Jordan Dalby",
  "version": "1.0.0",
  "main": "server/app.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "cd client && pnpm start",
    "server": "cd server && pnpm start",
    "dev": "docker-compose up --build"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "vite": "^5.4.10"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "bcrypt",
      "better-sqlite3"
    ]
  }
}
